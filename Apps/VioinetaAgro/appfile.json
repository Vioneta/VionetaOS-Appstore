    "container": {
        "image": "vioneta/vionetaos:latest",
        "shell": "bash",
        "privileged": false,
        "network_model": "host",
        "web_ui": {
            "http": "8123",
            "path": "/"
        },
        "envs": [
            {
                "key": "TZ",
                "value": "$TZ",
                "configurable": "no",
                "description": "TimeZone"
            },
            {
                "key": "PUID",
                "value": "$PUID",
                "configurable": "no",
                "description": "Run Vioneta Agro as specified uid."
            },
            {
                "key": "PGID",
                "value": "$PGID",
                "configurable": "no",
                "description": "Run Vioneta Agro as specified gid."
            }
        ],
        "ports": [],
        "volumes": [
            {
                "container": "/config",
                "host": "/DATA/AppData/$AppID/config",
                "mode": "rw",
                "allocation": "automatic",
                "configurable": "no",
                "description": "Vioneta Agro config directory."
            }
        ],
        "devices": [],
        "constraints": {
            "min_memory": 256,
            "min_storage": 1024
        },
        "restart_policy": "unless-stopped",
        "sysctls": [],
        "cap_add": [],
        "labels": []
    },
    "abilities": {
        "notification": false,
        "widgets": false,
        "authentication": false,
        "search": false,
        "upnp": false
    },
    "tips": {},
    "changelog":{
        "latest_updates": "",
        "url": ""
